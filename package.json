{
  "name": "EaselQA",
  "version": "1.0.0",
  "description": "EaselQA desktop app for mobile testcase recording.",
  "main": "build/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "gulp build",
    "start": "gulp start",
    "test": "gulp test",
    "release": "gulp release",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/mac.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . EaselQA --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/win.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"EaselQA\"",
    "package-linux": "electron-packager . EaselQA --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/linux.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/EaselQA-darwin-x64/EaselQA.app EaselQA --out=release-builds --overwrite --icon=assets/mac.icns",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "author": "Dataphion",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "electron": "^9.2.0",
    "electron-builder": "^20.39.0",
    "electron-connect": "^0.6.3",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.1.0",
    "electron-react-devtools": "^0.5.3",
    "electron-winstaller": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-css": "^0.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "react-test-renderer": "^16.8.6"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@wdio/cli": "^5.18.2",
    "antd": "^3.26.5",
    "axios": "^0.19.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bufferutil": "^4.0.1",
    "chromedriver": "^75.1.0",
    "electron-is-dev": "^1.1.0",
    "electron-promise-ipc": "^2.0.1",
    "electron-reload": "^1.4.1",
    "express": "^4.17.1",
    "gulp-livereload": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "lodash": "^4.17.15",
    "node-notifier": "^5.4.0",
    "puppeteer": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^4.2.0",
    "react-loader-spinner": "^3.1.5",
    "react-router-dom": "^5.0.1",
    "rsuite": "^4.2.0",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "socket.io-client": "^2.3.0",
    "sortablejs": "^1.10.2",
    "utf-8-validate": "^5.0.2",
    "xmldom": "^0.3.0",
    "xpath": "^0.0.27",
    "zeromq": "^5.2.0"
  },
  "build": {
    "appId": "com.dataphion.easelqa",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": null
  }
}
